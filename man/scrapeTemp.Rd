\examples{
##---- Should be DIRECTLY executable !! ----
##-- ==>  Define data, use random,
##--	or do  help(data=index)  for the standard data sets.

## The function is currently defined as
function (NLD_ids, cohort) 
{
    v <- vector()
    cohort <- cohort[, -grep("act", names(cohort))]
    for (i in NLD_ids) {
        v <- c(v, grep(i, names(cohort)))
    }
    CDR_trim <- cohort[, -v]
    CDR_trim <- data.frame(minutes = 1:nrow(CDR_trim), week = floor((CDR_trim$eday)/7) + 
        1, CDR_trim)
    NLD_trim <- cohort[, v]
    NLD_trim <- data.frame(minutes = 1:nrow(NLD_trim), week = floor((CDR_trim$eday)/7) + 
        1, cohort[, c(1, 2)], NLD_trim)
    tempDFs <- list(CDR = CDR_trim, NLD = NLD_trim)
    return(tempDFs)
  }
}
